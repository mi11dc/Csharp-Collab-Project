﻿@model TMS.Models.AgentVenueAssociationDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Venue?</h2>
<a href="/Venue">Cancel</a>
<h2>@Model.VenueDetail.Name</h2>
<form action="/Venue/Delete/@Model.Id" method="POST">
    <div style="display: none;">
        <input type="text" name="Id" value="@Model.Id" />
        <input type="text" name="AgentId" value="@Model.AgentId" />
        <input type="text" name="VenueId" value="@Model.VenueId" />
    </div>
    <input type="submit" class="btn btn-primary" value="Confirm Delete" />
</form>